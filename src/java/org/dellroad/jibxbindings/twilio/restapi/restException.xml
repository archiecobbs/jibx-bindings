<?xml version="1.0" encoding="ISO-8859-1"?>

<!-- $Id$ -->
<binding>
    <mapping name="RestException" class="org.dellroad.jibxbindings.twilio.restapi.RestException" ordered="false" flexible="true">
        <value name="Status" get-method="getStatus" set-method="setStatus"/>
        <value name="Message" get-method="getMessage" set-method="setMessage"/>
        <value name="Code" get-method="getCode" set-method="setCode" usage="optional"/>
        <value name="MoreInfo" get-method="getMoreInfo" set-method="setMoreInfo" usage="optional"
          deserializer="org.dellroad.jibxbindings.ParseUtil.deserializeURI"/>
    </mapping>
</binding>

