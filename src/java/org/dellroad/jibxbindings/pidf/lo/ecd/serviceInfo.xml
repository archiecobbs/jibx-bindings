<?xml version="1.0" encoding="ISO-8859-1"?>

<binding package="org.dellroad.jibxbindings.pidf.lo.ecd"
  xmlns:svc="urn:ietf:params:xml:ns:EmergencyCallData:ServiceInfo">
    <mapping abstract="true" type-name="svc:serviceInfo" class="org.dellroad.jibxbindings.pidf.lo.ecd.ServiceInfo"
      ordered="false" flexible="true" allow-repeats="true">
        <namespace uri="urn:ietf:params:xml:ns:EmergencyCallData:ServiceInfo" default="elements" prefix="svc"/>
        <value name="DataProviderReference" get-method="getDataProviderReference" set-method="setDataProviderReference"/>
        <value name="ServiceEnvironment" field="serviceEnvironment" usage="optional"/>
        <value name="ServiceType" type="java.lang.String" usage="optional"
          get-method="getServiceType" set-method="setServiceType"/>
        <value name="ServiceMobility" field="serviceMobility"/>
    </mapping>
</binding>
